{
  "name": "seo-copilot",
  "version": "3.0.0beta",
  "private": true,
  "main": "client/src/main.tsx",
  "type": "module",
  "scripts": {
    "build": "vite build && esbuild workers/index.ts --platform=node --packages=external --bundle --external:ip --external:ip-cidr --format=esm --outdir=public",
    "bundle-extension": "npx webflow-cli extension bundle || echo \"Skipping Webflow extension bundling - CLI not available\"",
    "dev": "concurrently -r \"npx webflow-cli extension serve || echo 'Webflow CLI not available, continuing...'\" \"vite --force\"",
    "start": "NODE_ENV=production node public/index.js",
    "check": "tsc",
    "dev:worker": "npx wrangler dev workers/index.ts --env development",
    "build:worker": "npx wrangler deploy workers/index.ts --env production",
    "build:all": "yarn build && yarn bundle-extension && yarn build:worker",
    "start-dev": "concurrently \"yarn dev\" \"yarn dev:worker\"",
    "prebuild": "yarn test",
    "test:coverage": "vitest run --coverage",
    "test": "vitest run",
    "test-worker": "vitest run tests/workers"
  },
  "devDependencies": {
